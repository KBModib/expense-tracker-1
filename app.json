{
  "expo": {
    "name": "All-Saver",
    "slug": "All-Saver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/appIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#D4F1F4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kbmodi.allsaver"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#D4F1F4"
      },
      "package": "com.kbmodi.allsaver"
    },
    "web": {
      "favicon": "./assets/appIcon.png"
    }
  }
}
